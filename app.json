{
  "expo": {
    "name": "ULC Electronics",
    "slug": "ULC",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/electronics.png",
    "scheme": "ulc",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/electronics.png",
        "backgroundColor": "#BF00FF"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.ULC"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/electronics.png",
          "imageWidth": 200,
          "resizeMode": "contain",
             "backgroundColor": "#8B5CF6"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
